                                "fp.company_id_text"
                            ]
                        },
                        "right_source": {
                            "identifier": "wh_db.DimCompany.companyid"
                        },
                        "join_condition": "(fp.company_id_text IS NOT NULL AND fp.company_id_text = dc.companyid::TEXT)"
                    }
