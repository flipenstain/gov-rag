{
    "sk_companyid": {
        "inputs": [
            {
                "column": "wh_db.DimCompany.effectivedate",
                "type": "INDIRECT",
                "subtype": "Join",
                "description": "fp.recdate >= dc.effectivedate",
                "masking": false
            },
            {
                "column": "wh_db.DimCompany.enddate",
                "type": "INDIRECT",
                "subtype": "Join",
                "description": "fp.recdate < dc.enddate",
                "masking": false
            },
            {
                "column": "finwire_parsed.company_id_text",
                "type": "INDIRECT",
                "subtype": "Join",
                "description": "fp.company_id_text IS NOT NULL AND fp.company_id_text = dc.companyid::TEXT",
                "masking": false
            },
            {
                "column": "wh_db.DimCompany.companyid",
                "type": "INDIRECT",
                "subtype": "Join",
                "description": "fp.company_id_text IS NOT NULL AND fp.company_id_text = dc.companyid::TEXT",
                "masking": false
            },
            {
                "column": "finwire_parsed.company_name",
                "type": "INDIRECT",
                "subtype": "Join",
                "description": "fp.company_name IS NOT NULL AND fp.company_name = dc.name",
                "masking": false
            },
            {
                "column": "wh_db.DimCompany.name",
                "type": "INDIRECT",
                "subtype": "Join",
                "description": "fp.company_name IS NOT NULL AND fp.company_name = dc.name",
                "masking": false
            },
            {
                "column": "wh_db.DimCompany.sk_companyid",
                "type": "DIRECT",
                "subtype": "Identity",
                "description": "Directly selected from DimCompany",
                "masking": false
            }
        ]
    },
    "fi_year": {
        "inputs": [
            {
                "column": "wh_db_stage.FinWire.value",
                "type": "DIRECT",
                "subtype": "Transformation",
                "description": "SUBSTRING(value FROM 1 FOR 4)",
                "masking": false
            }
        ],
        "transformation": "CAST(SUBSTRING(value FROM 1 FOR 4) AS INTEGER)"
    },
    "fi_qtr": {
        "inputs": [
